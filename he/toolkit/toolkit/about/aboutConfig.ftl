config-window =
    .title = about:config

## Strings used to display a warning in about:config

# This text should be attention grabbing and playful
config-about-warning-title =
    .value = פעולה זו עשויה לבטל את האחריות!
config-about-warning-button =
    .label = הסיכון מקובל עלי!
config-search-prefs =
    .value = חיפוש:
    .accesskey = ח
config-focus-search =
    .key = r

## These strings are used for column headers

config-pref-column =
    .label = שם העדפה
config-lock-column =
    .label = מצב
config-type-column =
    .label = סוג
config-value-column =
    .label = ערך

## These strings are used for tooltips

config-pref-column-header =
    .tooltip = לחץ לסידור

## These strings are used for the context menu

config-toggle =
    .label = החלף
    .accesskey = ה
config-reset =
    .label = אתחול
    .accesskey = א
config-new =
    .label = חדש
    .accesskey = ח
config-string =
    .label = מחרוזת
    .accesskey = ח
config-integer =
    .label = מספר שלם
    .accesskey = ס
config-boolean =
    .label = בוליאני
    .accesskey = ב
config-default = בררת מחדל
config-locked = נעול
config-property-string = מחרוזת
config-property-int = מספר שלם
config-property-bool = בוליאני
config-new-prompt = הכנס את שם ההעדפה
config-nan-title = ערך לא תקני
config-nan-text = הטקסט שהכנסת אינו מספר.
# Variables:
#   $type (String): type of value (boolean, integer or string)
config-new-title = ערך { $type } חדש
# Variables:
#   $type (String): type of value (boolean, integer or string)
config-modify-title = הכנס ערך { $type }
