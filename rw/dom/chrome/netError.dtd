<!ENTITY loadError.label "Ikosa mu gufungura paji">
<!ENTITY retry.label "Kongera kugerageza">
<!-- Specific error messages -->


<!ENTITY connectionFailure.title "Byanze kwinjira">
<!ENTITY connectionFailure.longDesc "<p>Nubwo urubuga rusa naho rwemewe, mucukumbuzi ntiyashoboye gushyiraho umwinjiro.</p><ul><li>Urubuga rwaba rudashobora kubona mu gihe gito? Kongera kugerageza hanyuma.</li><li>Ntabwo ushobora gushakisha izindi mbuga?  Kugenzura umwinjiro w'urusobemiyoboro wa mudasobwa.</li><li>Mudasobwa cyangwa urusobemiyoboro byawe birinzwe n'urukutamuriro cyangwa porogisi? Amagenamiterere atari yo ashobora kubangamira gushakisha ku Rubuga.</li></ul>">

<!ENTITY deniedPortAccess.title "Umuyoboro wabujijwe kubera impamvu z'umutekano">
<!ENTITY deniedPortAccess.longDesc "<p>Aderesi yasabwe yagaragaje umuyoboro (urugero <q>mozilla.org:80</q> w'umuyoboro 80 kuri mozilla.org) akenshi ukoreshwa hagamijwe  <em>ibindi</em> bitari gushakisha ku Rubuga. Mucukumbuzi yaretse ikibazo mu rwego rw'umutekano wawe no kukurinda.</p>">

<!ENTITY dnsNotFound.title "Aderesi ntiyabonetse">
<!ENTITY dnsNotFound.longDesc "<p>Mucukumbuzi ntiyashoboye kubona seriveri y'ubuturo kuri aderesi yatanzwe.</p><ul><li>Wakoze ikosa mu kwandika indangarubuga? (urugero <q><strong>ww</strong>.mozilla.org</q> mu mwanya wa <q><strong>www</strong>.mozilla.org</q>)</li><li>Uzi neza ko iyi ndangarubuga ibaho?  Icyemezo cyayo gishobora kuba cyararenze igihe.</li><li>Ntabwo ushobora gushakisha izindi mbuga?  Kugenzura amagenamiterere y'ukwihuza kw'urubosemiyoboro na seriveri ya DNS byawe.</li><li> Mudasobwa cyangwa urusobemiyoboro byawe birinzwe n'urukutamuriro cyangwa porogisi?  Amagenamiterere atari yo ashobora kubangamira gushakisha mu Rubuga.</li></ul>">

<!ENTITY fileNotFound.title "Dosiye ntibonetse">
<!ENTITY fileNotFound.longDesc "<ul><li>Igice gishobora kuba cyahinduwe izina, cyavanweho, cyangwa cyimuwe?</li><li>Haba hariho ikosa ry'imyandikire, inyuguti nkuru, cyangwa irindi kosa rw'imisusire muri aderesi?</li><li>Ufite ibyemezo byihagije zo kugera ku kintu cyasabwe?</li></ul>">

<!ENTITY generic.title "Ntibishoboka Kurangiza Ikibazo">
<!ENTITY generic.longDesc "<p>Ibisobanuro by'inyongera byerekeye iki kibazo cyangwa ikosa ntibishobora kuboneka ubu.</p>">

<!ENTITY malformedURI.title "Aderesi itemewe">
<!ENTITY malformedURI.longDesc "<p>Aderesi yatanzwe ntabwo ifite imiterere yemewe. Usabwe kugenzura umwanya w'indangahantu kugira ngo ubone amakosa maze wongere ugerageze.</p>">

<!ENTITY netInterrupt.title "Iyimura ry'Ibyatanzwe ryiburijwemo">
<!ENTITY netInterrupt.longDesc "<p>Mucukumbuzi yahujwe neza, ariko ukwihuza kwahagaritswe mu kwimura amakuru.  Usabwe kongera kugerageza.</p><ul><li>Ntabwo ushobora gushakisha  izindi mbuga? Kugenzura ukwihuza k'urusobemiyoboro rwa mudasobwa.</li><li>Uracyafite ingorane? Kubaza umuyobozi w'urusomiyoboro cyangwa utanga Interineti kugira ngo agufashe.</li></ul>">

<!ENTITY notCached.title "Document Expired">
<!ENTITY notCached.longDesc "<p>The requested document is not available in the browser's cache.</p><ul><li>As a security precaution, the browser does not automatically re-request sensitive documents.</li><li>Click Try Again to re-request the document from the website.</li></ul>">

<!ENTITY netOffline.title "Buryo nta interineti">
<!ENTITY netOffline.longDesc2 "<p>The browser is operating in its offline mode and cannot connect to the requested item.</p><ul><li>Is the computer connected to an active network?</li><li>Press &quot;Try Again&quot; to switch to online mode and reload the page.</li></ul>">

<!ENTITY contentEncodingError.title "Content Encoding Error">
<!ENTITY contentEncodingError.longDesc "<p>The page you are trying to view cannot be shown because it uses an invalid or unsupported form of compression.</p><ul><li>Please contact the website owners to inform them of this problem.</li></ul>">

<!ENTITY unsafeContentType.title "Unsafe File Type">
<!ENTITY unsafeContentType.longDesc "
<ul>
  <li>Please contact the website owners to inform them of this problem.</li>
</ul>
">

<!ENTITY netReset.title "Kwihuza kwaburijwemo">
<!ENTITY netReset.longDesc "<p>Ihuza ry'urusobemiyoboro ryahagaritswe mu gihe cyo gushakisha umwinjiro. Usabwe kongera kugerageza.</p>">

<!ENTITY netTimeout.title "Kurangiza igihe kuri interineti">
<!ENTITY netTimeout.longDesc "<p>Urubuga rwasabwe ntirwasubije ikibazo cyo kwihuza na mucukumbuzi yaretse gutegereza igisubizo.</p><ul><li>Seriveri ishobora kuba yahawe ibibazo byinshi cyangwa se ikaba yihagaritse mu gihe gito?  Kongera kugerageza hanyuma.</li><li>Ntabwo ushobora gushakisha izindi mbuga? Kugenzura umwinjiro w'urusobemiyoboro wa mudasobwa.</li><li>Mudasobwa cyangwa urusobemiyoboro byawe birinzwe n'urukutamuriro cyangwa porogisi? Amagenamiterere atari yo ashobora kubangamira gushakisha ku Rubuga.</li><li>Uracyafite ingorane? Kubaza umuyobozi w'urusobemiyoboro wawe cyangwa utanga Interineti kugira ngo agufashe.</li></ul>">

<!ENTITY protocolNotFound.title "Porotokole itazwi">
<!ENTITY protocolNotFound.longDesc "<p>Aderesi igaragaza porotokole (urugero <q>wxyz://</q>) mucukumbuzi itemera, bityo mucukumbuki ntishobora kwinjira neza ku rubuga.</p><ul><li>Urimo kugerageza kugera kuri mutwaramakuru cyangwa izindi serivisi zitari inyandiko? Kugenzura urubuga kugira ngo ubone ibisabwa bindi.</li><li>Porotokole zimwe na zimwe zishobora gusaba porogaramu cyangwa amacomeka yo hanze mbere y'uko mucukumbuzi ishobora kuyemera.</li></ul>">

<!ENTITY proxyConnectFailure.title "Seriveri ya Porogisi Yanze Umwinjiro">
<!ENTITY proxyConnectFailure.longDesc "<p>Mucukumbuzi yahawe imiterere ituma ikoresha seriveri ya porogisi, ariko porogisi yanze kwihuza.</p><ul><li>Igenamiterere rya porogisi ya seriveri ni ryo? Kugenzura amagenamiterere maze ukongera kugerageza.</li><li>Serivisi ya porogisi yaba yemera imyinjiro ihereye kuri uru rusobemiyoboro?</li><li>Uracyafite ingorane? Kubaza umuyobozi w'urusobemiyoboro cyangwa utanga Interineti kugira ngo agufashe.</li></ul>">

<!ENTITY proxyResolveFailure.title "Seriveri porogisi ntibonetse">
<!ENTITY proxyResolveFailure.longDesc "<p>Mucukumbuzi yahawe imiterere ituma ikoresha seriveri ya porogisi, ariko porogisi ntiyashoboye kuboneka.</p><ul><li>Ibonezamiterere rya porogisi ya mucukumbuzi ni ryo? Kugenzura amagenamiterere maze ukongera kugerageza.</li><li>Mudasobwa yaba iri ku rusobemiyoboro rukora?</li><li>Uracyafite ingorane? Kubaza umuyobozi w'urusobemiyoboro rwawe cyangwa utanga Interineti kugira ngo agufashe.</li></ul>">

<!ENTITY redirectLoop.title "Igaruka kenshi ry'iyobora">
<!ENTITY redirectLoop.longDesc "<p>Mucukumbuzi yaretse kugerageza kuzana ikintu cyasabwe. Urubuga rurimo kongera kohereza ikibazo mu buryo budashobora kurangira.</p><ul><li>Wahagaritse cyangwa wafunze Dosiyenyibutsa zisabwa n'uru rubuga?</li><li><em>NOTE</em>: Niba kwemera Dosiyenyibutsa z'urubuga bidakemura ikibazo, bishobora kuba ari ikibazo cy'igenamiterere rya seriveri aho kuba icya mudasobwa yawe.</li></ul>">

<!ENTITY unknownSocketType.title "Igisubizo si cyo">
<!ENTITY unknownSocketType.longDesc "<p>Urubuga rwasubije ikibazo cy'urusobemiyoboro mu buryo butunguranye bityo mucukumbuzi ntishobora gukomeza.</p>">

<!ENTITY nssFailure2.title "Secure Connection Failed">
<!ENTITY nssFailure2.longDesc "<p>The page you are trying to view cannot be shown because the authenticity of the received data could not be verified.</p><ul><li>Please contact the website owners to inform them of this problem.</li></ul>">

<!ENTITY nssBadCert.title "Secure Connection Failed">
<!ENTITY nssBadCert.longDesc2 "<ul>
<li>This could be a problem with the server's configuration, or it could be someone trying to impersonate the server.</li>
<li>If you have connected to this server successfully in the past, the error may be temporary, and you can try again later.</li>
</ul>
">

<!ENTITY malwareBlocked.title "Suspected Attack Site!">
<!ENTITY malwareBlocked.longDesc "
<p>Attack sites try to install programs that steal private information, use your computer to attack others, or damage your system.</p>
<p>Website owners who believe their site has been reported as an attack site in error may <a href='http://www.stopbadware.org/home/reviewinfo' >request a review</a>.</p>
">

<!ENTITY phishingBlocked.title "Suspected Web Forgery!">
<!ENTITY phishingBlocked.longDesc "
<p>Entering any personal information on this page may result in identity theft or other fraud.</p>
<p>These types of web forgeries are used in scams known as phishing attacks, in which fraudulent web pages and emails are used to imitate sources you may trust.</p>
">

<!ENTITY cspFrameAncestorBlocked.title "Blocked by Content Security Policy">
<!ENTITY cspFrameAncestorBlocked.longDesc "<p>The browser prevented this page from loading in this way because the page has a content security policy that disallows it.</p>">

<!ENTITY corruptedContentError.title "Corrupted Content Error">
<!ENTITY corruptedContentError.longDesc "<p>The page you are trying to view cannot be shown because an error in the data transmission was detected.</p><ul><li>Please contact the website owners to inform them of this problem.</li></ul>">

<!ENTITY remoteXUL.title "Remote XUL">
<!ENTITY remoteXUL.longDesc "<p><ul><li>Please contact the website owners to inform them of this problem.</li></ul></p>">
