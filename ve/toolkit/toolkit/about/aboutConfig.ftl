config-window =
    .title = about:config

## Strings used to display a warning in about:config


## These strings are used for column headers

config-pref-column =
    .label = Dzina ḽi Takalelwaho
config-lock-column =
    .label = Tshiimo
config-type-column =
    .label = Lushakka
config-value-column =
    .label = Ndeme

## These strings are used for tooltips

config-pref-column-header =
    .tooltip = Kiḽikani u itela u dzudzanya
config-column-chooser =
    .tooltip = Kiḽikani u itela u nanga khoḽomu dza u ṱana

## These strings are used for the context menu

config-copy-name =
    .label = Kopani Dzina
    .accesskey = K

config-copy-value =
    .label = Kopani Ndeme
    .accesskey = N

config-modify =
    .label = Lulamisani
    .accesskey = L

config-toggle =
    .label = Pfundo
    .accesskey = P

config-reset =
    .label = Sethani hafhu
    .accesskey = S

config-new =
    .label = Ntswa
    .accesskey = N

config-string =
    .label = Mutevhe
    .accesskey = M

config-integer =
    .label = Nomboro
    .accesskey = N

config-boolean =
    .label = Boolean
    .accesskey = B

config-default = u iledzwa
config-locked = yo khiiwa

config-property-string = mutevhe
config-property-int = mbalosia
config-property-bool = boolean

config-new-prompt = Dzhenisani dzina ḽi takalelwaho

# Variables:
#   $type (String): type of value (boolean, integer or string)
config-new-title = Ndeme ya { $type } ntswa

# Variables:
#   $type (String): type of value (boolean, integer or string)
config-modify-title = Dzhenisani ndeme ya { $type }
