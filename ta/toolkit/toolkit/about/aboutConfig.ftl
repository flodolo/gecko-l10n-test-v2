config-window =
    .title = about:config

## Strings used to display a warning in about:config


## These strings are used for column headers

config-lock-column =
    .label = நிலை
config-type-column =
    .label = வகை
config-value-column =
    .label = மதிப்பு

## These strings are used for tooltips


## These strings are used for the context menu

config-copy-pref =
    .key = C
    .label = நகலெடு
    .accesskey = C
config-string =
    .label = சரம்
    .accesskey = S
config-boolean =
    .label = பூலியன்
    .accesskey = B
config-default = இயல்பான
config-locked = பூட்டப்பட்ட
config-property-string = சரம்
config-property-int = இயல் எண்
config-property-bool = பூலியன்
config-new-prompt = விருப்ப பெயரை உள்ளிடவும்
# Variables:
#   $type (String): type of value (boolean, integer or string)
config-new-title = புதிய { $type } மதிப்பு
# Variables:
#   $type (String): type of value (boolean, integer or string)
config-modify-title = மதிப்பை { $type } உள்ளிடவும்
