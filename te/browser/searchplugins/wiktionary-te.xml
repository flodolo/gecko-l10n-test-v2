<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Wiktionary (te)</ShortName>
<Description>Wiktionary (te)</Description>
<Image height="16" width="16" type="image/x-icon">http://te.wiktionary.org/favicon.ico</Image>
<Url type="text/html" method="get" template="http://te.wiktionary.org/wiki/ప్రత్యేక:అన్వేషణ" resultdomain="wiktionary.org">
  <Param name="search" value="{searchTerms}"/>
</Url>
<Url type="application/x-suggestions+json" method="GET" template="http://te.wiktionary.org/w/api.php">
  <Param name="action" value="opensearch"/>
  <Param name="search" value="{searchTerms}"/>
  <Param name="namespace" value="0"/>
</Url>
<SearchForm>http://te.wiktionary.org/</SearchForm>
</SearchPlugin>
