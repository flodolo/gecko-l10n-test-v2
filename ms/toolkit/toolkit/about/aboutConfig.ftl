config-window =
    .title = about:config

## Strings used to display a warning in about:config

# This text should be attention grabbing and playful
config-about-warning-title =
    .value = Ini mungkin membatalkan jaminan anda!
config-about-warning-button =
    .label = Saya terima risikonya!
config-focus-search =
    .key = r

## These strings are used for column headers

config-pref-column =
    .label = Nama Keutamaan
config-lock-column =
    .label = Status
config-type-column =
    .label = Jenis
config-value-column =
    .label = Nilai

## These strings are used for tooltips

config-column-chooser =
    .tooltip = Klik untuk pilih kolum dipapar

## These strings are used for the context menu

config-copy-name =
    .label = Salin Nama
    .accesskey = N
config-integer =
    .label = Integer
    .accesskey = I
config-boolean =
    .label = Boolean
    .accesskey = B
config-locked = dikunci
config-property-int = integer
config-property-bool = boolean
config-new-prompt = Masukkan nama keutamaan
config-nan-title = Nilai tidak sah
config-nan-text = Teks yang anda masukkan bukan berbentuk nombor.
# Variables:
#   $type (String): type of value (boolean, integer or string)
config-new-title = { $type } nilai baru
# Variables:
#   $type (String): type of value (boolean, integer or string)
config-modify-title = Masukkan { $type } nilai
