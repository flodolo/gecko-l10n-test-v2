welcome-back-tab-title = הצלחה!
welcome-back-page-title = הדפדפן מוכן ומזומן לפקודתך!
welcome-back-page-info = { -brand-short-name } מוכן כעת לנסיעה.
welcome-back-restore-button =
    .label = דרך צלחה!
    .accesskey = ד
